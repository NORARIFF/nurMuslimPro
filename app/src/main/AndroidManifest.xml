<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nurmuslim">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".List10"></activity>
        <activity android:name=".List9" />
        <activity android:name=".List8" />
        <activity android:name=".List7" />
        <activity android:name=".List6" />
        <activity android:name=".List5" />
        <activity android:name=".List4" />
        <activity android:name=".List3" />
        <activity android:name=".List2" />
        <activity android:name=".ProductList" />
        <activity android:name=".Location" />
        <activity android:name=".Scanners" />

        <activity android:name=".Search" />
        <activity android:name=".AboutUs" />
        <activity android:name=".Home" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CaptureAct"
            android:screenOrientation="fullSensor"
            android:stateNotNeeded="true"
            android:windowSoftInputMode="stateAlwaysHidden"></activity>

    </application>

</manifest>